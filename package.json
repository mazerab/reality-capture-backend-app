{
  "name": "reality-capture-backend-app",
  "version": "1.0.0",
  "description": "AWS Lambda backend for Autodesk Forge Reality Capture Mobile App.",
  "main": "app.js",
  "files": [
    "*.js"
  ],
  "scripts": {
    "create": "claudia create --handler lambda.handler --deploy-proxy-api --region us-east-1 -name recap-forge-service --version demo",
    "setvars": "claudia set-version --version demo --set-env AWS_ACCESS_KEY_ID=xxx,AWS_SECRET_ACCESS_KEY=xxx",
    "destroy": "claudia destroy",
    "test": "echo \"Error: no test specified\" && exit 1",
    "update": "claudia update --version demo"
  },
  "author": "Bastien Mazeran",
  "license": "MIT",
  "dependencies": {
    "aws-sdk": "^2.209.0",
    "aws-serverless-express": "^3.1.3",
    "express": "^4.16.2",
    "fetch": "^1.1.0",
    "request": "^2.85.0"
  },
  "devDependencies": {
    "claudia": "^2.1.0",
    "eslint-plugin-flowtype": "^2.46.1"
  }
}
